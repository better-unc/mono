{
  "name": "gitbruv",
  "devDependencies": {
    "prettier": "^3.8.0",
    "turbo": "^2.7.2",
    "typescript": "^5"
  },
  "overrides": {
    "better-auth": "^1.4.12",
    "@better-auth/expo": "^1.4.12",
    "typescript": "^5",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.10",
    "@tanstack/react-query": "^5.90.16",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "drizzle-orm": "^0.45.1",
    "postgres": "^3.4.8",
    "bcryptjs": "^3.0.3",
    "@types/bcryptjs": "^3.0.0"
  },
  "packageManager": "bun@1.3.5",
  "private": true,
  "scripts": {
    "dev": "turbo dev --filter=@gitbruv/web --filter=@gitbruv/api",
    "dev:web": "turbo dev --filter=@gitbruv/web",
    "dev:api": "turbo dev --filter=@gitbruv/api",
    "dev:mobile": "turbo dev --filter=@gitbruv/mobile --filter=@gitbruv/web",
    "build": "turbo build",
    "build:web": "turbo build --filter=@gitbruv/web",
    "build:api": "turbo build --filter=@gitbruv/api",
    "build:mobile": "turbo build --filter=@gitbruv/mobile",
    "start:web": "turbo start --filter=@gitbruv/web",
    "start:api": "turbo start --filter=@gitbruv/api",
    "start:mobile": "turbo start --filter=@gitbruv/mobile",
    "lint": "turbo lint",
    "db:generate": "turbo db:generate --filter=@gitbruv/db",
    "db:migrate": "turbo db:migrate --filter=@gitbruv/db",
    "db:push": "turbo db:push --filter=@gitbruv/db",
    "db:studio": "turbo db:studio --filter=@gitbruv/db"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
